#include<stdio.h>
int main()
{
	double first,second,temp;
	printf("enter first number:");
	scanf("%f",&first);
	printf("enter second number:");
	scanf("%f",&second);
	temp=first;
	first=second;
	second=temp;
	printf("after swapping, first number =%f\n",first);
	printf("after swapping, second number =%f\n",second);
	return 0;
}
