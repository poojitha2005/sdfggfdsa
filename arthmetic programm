#include <stdio.h>
int main()
{
    int num1, num2;
    int sum, pow, sub, mult, mod;
    float div;
    printf("Enter any two numbers: ");
    scanf("%d%d", &num1, &num2);
    sum = num1 + num2;
    pow = num1 ^ num2;
    sub = num1 - num2;
    mult = num1 * num2;
    div = (float)num1 / num2;
    mod = num1 % num2;

   
    printf("Sum = %d\n", sum);
    printf("pow = %d\n",pow);
    printf("Diffrence = %d\n", sub);
    printf("multiply = %d\n", mult);
    printf("Quotient = %f\n", div);
    printf("Modulus = %d", mod);

    return 0; 
}
